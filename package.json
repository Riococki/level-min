{
  "name": "level-min",
  "version": "0.1.0",
  "description": "A light-weight full-text search library for the browser and Node.js, aimed to minimize the workload of developers in usage. Using LevelDB as storage backend.",
  "main": "lib/main.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ruiyeah/level-min.git"
  },
  "keywords": [
    "leveldb"
  ],
  "scripts": {
    "test": "babel src -d lib && node test.js",
    "start": "webpack-dev-server --hot --inline --content-base --config webpack.config.js",
    "pack": "webpack --mode development",
    "build": "webpack --mode production",
    "compile": "babel src -d lib"
  },
  "author": "rui",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ruiyeah/level-min/issues"
  },
  "engines": {
    "node": ">=6"
  },
  "homepage": "https://github.com/ruiyeah/level-min#readme",
  "dependencies": {
    "deasync": "^0.1.19",
    "franc": "^5.0.0",
    "langjudge": "^0.3.0",
    "level": "^6.0.0",
    "md5": "^2.2.1",
    "natural": "^0.6.3",
    "segmentit": "^2.0.3",
    "stopword": "^0.3.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3"
  }
}
